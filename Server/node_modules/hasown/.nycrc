// Configuration object for code coverage report
{
  // Include code coverage information for all files
  "all": true,

  // Do not enforce a minimum code coverage threshold
  "check-coverage": false,

  // Array of reporters to generate the code coverage report
  "reporter": [
    "text-summary", // Summary of code coverage in text format
    "text", // Detailed code coverage report in text format
    "html", // Detailed code coverage report in HTML format
    "json" // Detailed code coverage report in JSON format
  ],

  // Code coverage metrics
  "lines": 86, // 86
